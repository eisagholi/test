<?xml version="1.0" encoding="utf-8"?>
<Container version="2">
    <TemplateURL>https://raw.githubusercontent.com/eisagholi/test/master/handbrake.xml</TemplateURL>
    <Beta>False</Beta>
    <Category>MediaApp:Video</Category>
    <Name>handbrake</Name>
    <Support>https://github.com/eisagholi/docker-templates/issues</Support>
    <Overview>HandBrake is a tool for converting video from nearly any format to a selection of modern, widely supported codecs.</Overview>
    <Registry>https://hub.docker.com/r/jlesage/handbrake</Registry>
    <Repository>jlesage/handbrake</Repository>
    <Icon>https://raw.githubusercontent.com/eisagholi/test/master/img/handbrake_icon.png</Icon>
    <Privileged>false</Privileged>
    <BindTime>true</BindTime>
    <WebUI></WebUI>
    <Network>traefik</Network>
    <Config Name="AppData Config Path" Target="/config" Default="/mnt/user/appdata/handbrake" Mode="rw" Description="Container Path: /config" Type="Path" Display="advanced-hide" Required="true" Mask="false">/mnt/user/appdata/handbrake</Config>
    <Config Name="Home Folder Path" Target="/storage" Default="/mnt/user/" Mode="ro" Description="Container Path: /storage" Type="Path" Display="advanced-hide" Required="true" Mask="false">/mnt/user/</Config>
    <Config Name="Usenet Watch Folder Path" Target="/watch" Default="/mnt/user/downloads/usenet/completed" Mode="rw" Description="Container Path: /watch" Type="Path" Display="advanced-hide" Required="true" Mask="false">/mnt/user/downloads/usenet/completed</Config>
    <Config Name="Media Processing Folder Path" Target="/output" Default="/mnt/user/downloads/media_postprocessing" Mode="rw" Description="Container Path: /output" Type="Path" Display="advanced-hide" Required="true" Mask="false">/mnt/user/downloads/media_postprocessing</Config>
    <Config Name="USER_ID" Target="USER_ID" Default="100" Mode="" Description="Container Variable: USER_ID" Type="Variable" Display="always" Required="false" Mask="false">100</Config>
    <Config Name="GROUP_ID" Target="GROUP_ID" Default="1000" Mode="" Description="Container Variable: GROUP_ID" Type="Variable" Display="always" Required="false" Mask="false">1000</Config>
    <Config Name="AUTOMATED_CONVERSION_PRESET" Target="AUTOMATED_CONVERSION_PRESET" Default="myh265" Mode="" Description="Container Variable: AUTOMATED_CONVERSION_PRESET" Type="Variable" Display="always" Required="false" Mask="false">myh265</Config>
    <Config Name="AUTOMATED_CONVERSION_FORMAT" Target="AUTOMATED_CONVERSION_FORMAT" Default="mkv" Mode="" Description="Container Variable: AUTOMATED_CONVERSION_FORMAT" Type="Variable" Display="always" Required="false" Mask="false">mkv</Config>
    <Config Name="AUTOMATED_CONVERSION_KEEP_SOURCE" Target="AUTOMATED_CONVERSION_KEEP_SOURCE" Default="0" Mode="" Description="Container Variable: AUTOMATED_CONVERSION_KEEP_SOURCE" Type="Variable" Display="always" Required="false" Mask="false">0</Config>
    <Config Name="AUTOMATED_CONVERSION_OUTPUT_SUBDIR" Target="AUTOMATED_CONVERSION_OUTPUT_SUBDIR" Default="SAME_AS_SRC" Mode="" Description="Container Variable: AUTOMATED_CONVERSION_OUTPUT_SUBDIR" Type="Variable" Display="always" Required="false" Mask="false">SAME_AS_SRC</Config>
    <Config Name="traefik.enable" Target="traefik.enable" Default="true" Mode="" Description="Container Label: traefik.enable" Type="Label" Display="always" Required="false" Mask="false">true</Config>
    <Config Name="handbrake-rtr.tls" Target="traefik.http.routers.handbrake-rtr.tls" Default="true" Mode="" Description="Container Label: traefik.http.routers.handbrake-rtr.tls" Type="Label" Display="always" Required="false" Mask="false">true</Config>
    <Config Name="handbrake-rtr.entrypoints" Target="traefik.http.routers.handbrake-rtr.entrypoints" Default="https" Mode="" Description="Container Label: traefik.http.routers.handbrake-rtr.entrypoints" Type="Label" Display="always" Required="false" Mask="false">https</Config>
    <Config Name="handbrake-rtr.rule" Target="traefik.http.routers.handbrake-rtr.rule" Default="Host(`handbrake.example.com`)" Mode="" Description="Container Label: traefik.http.routers.handbrake-rtr.rule" Type="Label" Display="always" Required="false" Mask="false">Host(`handbrake.example.com`)</Config>
    <Config Name="docker.network" Target="traefik.docker.network" Default="" Mode="" Description="Container Label: traefik.docker.network" Type="Label" Display="always" Required="false" Mask="false">traefik</Config>
    <Config Name="handbrake-rtr.service" Target="traefik.http.routers.handbrake-rtr.service" Default="handbrake-svc" Mode="" Description="Container Label: traefik.http.routers.handbrake-rtr.service" Type="Label" Display="always" Required="false" Mask="false">handbrake-svc</Config>
    <Config Name="handbrake-svc.loadbalancer.server.port" Target="traefik.http.services.handbrake-svc.loadbalancer.server.port" Default="5800" Mode="" Description="Container Label: traefik.http.services.handbrake-svc.loadbalancer.server.port" Type="Label" Display="always" Required="false" Mask="false">5800</Config>
    <Config Name="traefik.http.routers.handbrake-rtr.middlewares" Target="traefik.http.routers.handbrake-rtr.middlewares" Default="chain-no-auth@file" Mode="" Description="Container Label: traefik.http.routers.handbrake-rtr.middlewares" Type="Label" Display="always" Required="false" Mask="false">chain-no-auth@file</Config>
</Container>
